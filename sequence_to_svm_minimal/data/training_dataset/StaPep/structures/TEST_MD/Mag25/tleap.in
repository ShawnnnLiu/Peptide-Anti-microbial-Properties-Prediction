source leaprc.protein.ff14SB
source leaprc.water.tip3p
set default PBRadii mbondi3
loadAmberPrep /mnt/c/Users/bioin/Documents/SVM_ESM_Peptides/stapep_package/stapep/templates/PS5/ps5.prepin
loadAmberParams /mnt/c/Users/bioin/Documents/SVM_ESM_Peptides/stapep_package/stapep/templates/PS5/frcmod.ps5
pep = sequence { PS5 LYS GLY LYS PS5 LEU HIS SER LYS LYS LYS PHE GLY LYS ALA PS5 VAL GLY GLU PS5 ALA LYS LYS }
bond pep.1.C2 pep.5.C2
